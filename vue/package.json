    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vuesp-components": "^0.2.8",
    "vuesp-struct": "^1.0.0",
    "vuesp-data": "^1.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.5",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vue/tsconfig": "^0.7.0",
    "minify": "^9.1.0",
    "node-gzip": "^1.1.2",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.1",
    "vite-plugin-compression2": "^1.3.3",
    "vue-tsc": "^2.2.8"
