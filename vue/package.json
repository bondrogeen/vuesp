{
  "name": "vuesp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preview": "vite preview",
    "build": "npm run increment && npm run recovery && npm run parser && vue-tsc -b && vite build && npm run firmware",
